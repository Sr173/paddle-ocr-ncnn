{
  "name": "paddle-ocr-ncnn",
  "version": "1.0.0",
  "description": "Node.js bindings for PaddleOCR using ncnn inference engine",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "gypfile": true,
  "scripts": {
    "preinstall": "node scripts/download-deps.js",
    "install": "node-gyp rebuild",
    "build": "node-gyp build",
    "rebuild": "node-gyp rebuild",
    "clean": "node-gyp clean",
    "build:debug": "node-gyp rebuild --debug",
    "test": "node test/test.js"
  },
  "keywords": [
    "ocr",
    "paddle",
    "paddleocr",
    "ncnn",
    "text-recognition",
    "native",
    "node-addon",
    "vulkan"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "node-gyp": "^10.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "binary": {
    "napi_versions": [8]
  },
  "files": [
    "lib/",
    "src/",
    "scripts/",
    "binding.gyp"
  ]
}
